{
	"schemaVersion": 1,
	"id": "fireworkextensions",
	"version": "${version}",
	"name": "Firework Extensions",
	"description": "A Minecraft Fabric Library to add custom firework effects.",
	"authors": [
		"NatsuCamellia"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/NatsuCamellia/Firework-Extensions/"
	},
	"license": "MIT",
	"icon": "assets/fireworkextensions/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.natsucamellia.fireworkextensions.FireworkExtensions"
		],
		"fabric-datagen": [
			"net.natsucamellia.fireworkextensions.FireworkExtensionsDataGenerator"
		]
	},
	"mixins": [
		"fireworkextensions.mixins.json"
	],
	"accessWidener" : "fireworkextensions.accesswidener",
	"depends": {
		"fabricloader": ">=0.15.6",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}